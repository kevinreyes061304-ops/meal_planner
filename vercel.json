{
  "version": 2,
  "builds": [
    {
      "src": "meal_planner/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "meal_planner/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "meal_planner.settings"
  }
}